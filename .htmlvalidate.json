{
  "extends": ["html-validate:recommended"],
  "rules": {
    "void-style": ["error", {"style": "omit"}],
    "close-order": "error",
    "doctype-html": "error",
    "no-missing-references": "error",
    "require-sri": "off",
    "no-inline-style": "warn",
    "prefer-native-element": "error",
    "no-redundant-role": "error",
    "wcag/h37": "error",
    "wcag/h67": "error"
  },
  "elements": [
    "html5"
  ]
}
